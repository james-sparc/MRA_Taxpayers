@{
    Layout = null;
}<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>MRA | Taxpayers</title>
  <!-- Favicon -->
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/styling/img/svg/logomra.png" type="image/x-icon">
  <!-- Custom styles -->
  <link rel="stylesheet" href="~/styling/css/style.min.css">
</head>

<body>
  <div class="layer"></div>
<main class="page-center">
   
     <img src="~/styling/img/svg/mra_pic.jpg" class="w3-image image-responsive w3-circle" style="width:150px;height:100px" />
  <article class="sign-up">
    <h1 class="sign-up__title">Welcome</h1>
    <p class="sign-up__subtitle">Log In</p>
    <form class="sign-up-form form" action="@Url.Action("Login","Account")" method="post">
       <div asp-validation-summary="ModelOnly" class="text-danger"></div>

      <label class="form-label-wrapper">
        <p class="form-label">Email</p>
        <input class="form-input" name="email" type="email" placeholder="Enter your email" required>
      </label>
      <label class="form-label-wrapper">
        <p class="form-label">Password</p>
        <input class="form-input" name="password" type="password" placeholder="Enter your password" required>
      </label>
    
     
      <button class="form-btn primary-default-btn transparent-btn">Log In</button>
    </form>
  </article>
</main>
<!-- Chart library -->
<script src="~/styling/plugins/chart.min.js"></script>
<!-- Icons library -->
<script src="~/styling/plugins/feather.min.js"></script>
<!-- Custom scripts -->
<script src="~/styling/js/script.js"></script>
</body>

</html>